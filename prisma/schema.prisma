generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           String @id @default(uuid())
  name         String @db.VarChar(63)
  email        String @unique @db.VarChar(127)
  phone_number String
  password     String @db.VarChar(127)
  birthday_at  String

  @@map("users")
}
